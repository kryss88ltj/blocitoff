<h1><%= current_user.name%>'s Lists</h1>

<% @lists.each do |list| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to list.title, list %>
          </h4>
        </div>
      </div>
    <% end %>


<%= link_to "New List", new_list_path(@list), class: 'btn btn-success' %>